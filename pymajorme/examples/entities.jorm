// This is the referent example

package pymagen.model

entity Element {
    Integer id : GUID, name=id
    String name : unique=true, nullable=false
    String description : nullable=true
}

entity ElementType {
    Integer id : GUID
    String name : nullable = false, unique=true
    String description
}

ElementType -- Element
ElementType -> Element : cascade = ALL, fetch = LAZY
ElementType <-> Element
